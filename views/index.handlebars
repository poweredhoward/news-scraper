{{#each results}}

<div class="panel panel-default">
   

        <div class="panel-heading">
            <a href={{link}} target="_blank"><h3 class="panel-title">{{title}}</h3></a>
        </div>

        
        
            
            <div class="panel-body">
                <div class="container-row">
                    <div class="row">
                        <div class="col-md-7">
                            {{summary}}
                        </div>
                         <div class="col-md-3">
                            <img class="center-block" src={{picture}}>
                        </div>
                        <div class="col-md-2">
                            <button class="btn btn-primary note" data-id={{_id}}>Make A Note</button>
                            <button class="btn btn-info viewNotes" data-id={{_id}}>View Notes</button>
                        </div>
                    </div>
                </div>
            </div>
        

       
    </div>
    
    
</div>
{{/each}}